{
  "name": "STM32F100R8T6B",
  "url": "https://github.com/Wiz-IO/wizio-stm",
  "vendor": "WizIO",
  "frameworks": [ "WizIO-STM32-SDK" ],

  "build": {
    "div"               : "STM32F1",
    "sub"               : "%00xB",    
    "mcu"               : "%00R8Tx",
    "cor"               : [ "-march=armv7-m", "-mcpu=cortex-m3", "-mthumb" ],
    
    "f_cpu"             : "24000000L"
  },

  "debug": {
    "default_tools": [
      "stlink"
    ],
    "jlink_device": "STM32F100R8T6B",
    "onboard_tools": [
      "stlink"
    ],
    "openocd_target": "stm32f1",
    "svd_path": "STM32F100xx.svd"
  },

  "upload": {
    "maximum_ram_size"  :  8192,
    "maximum_size"      : 65536,
    "protocol": "stlink",
    "protocols": [
      "jlink",
      "stlink"
    ]    
  }
}
